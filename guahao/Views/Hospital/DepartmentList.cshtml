@model IEnumerable<guahao.Models.department>

@{
    ViewBag.Title = "DepartmentList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>DepartmentList</h2>
<div class="show">
    @foreach (var department in Model)
    {
        <div class="col-md-4 jumbotron">
            @Html.ActionLink(@department.name, "DepartmentDetail/" + @department.id, "Hospital")
        </div>

    }
</div>